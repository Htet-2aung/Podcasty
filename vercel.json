{
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "https://itunes.apple.com/search"
    },
    {
      "source": "/api/lookup",
      "destination": "https://itunes.apple.com/lookup"
    },
    {
      "source": "/api/us/rss/:path*",
      "destination": "https://itunes.apple.com/us/rss/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
